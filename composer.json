{
    "name": "makelarisjr/laravel-2fa",
    "license": "MIT",
    "keywords": ["laravel", "authentication", "2fa", "ubikey", "otp", "totp"],
    "autoload": {
        "psr-4": {
            "MakelarisJR\\Laravel2FA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakelarisJR\\Laravel2FA\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "makelarisjr",
            "email": "makelarisjr@hackthebox.eu"
        },
        {
            "name": "Meletis Flevarakis",
            "email": "meletis@flevarakis.xyz"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/google2fa": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nunomaduro/larastan": "^0.7.12",
        "nunomaduro/collision": "^5.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MakelarisJR\\Laravel2FA\\Laravel2FAServiceProvider"
            ],
            "aliases": {
                "Laravel2FA": "MakelarisJR\\Laravel2FA\\Facades\\Laravel2FA"
            }
        }
    }
}
